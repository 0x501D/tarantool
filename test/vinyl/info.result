env = require('test_run')
---
...
test_run = env.new()
---
...
test_run:cmd('create server vinyl_info with script="vinyl/vinyl_info.lua"')
---
- true
...
test_run:cmd("start server vinyl_info")
---
- true
...
test_run:cmd('switch vinyl_info')
---
- true
...
space = box.schema.space.create('test', { engine = 'vinyl' })
---
...
index = space:create_index('primary', { type = 'tree', parts = {1, 'str'} })
---
...
space:replace({'xxx'})
---
- ['xxx']
...
space:get({'xxx'})
---
- ['xxx']
...
space:select()
---
- - ['xxx']
...
space:delete({'xxx'})
---
...
test_run:cmd("setopt delimiter ';'")
---
- true
...
for _, v in ipairs({ 'path', 'build', 'tx_latency', 'cursor_latency',
                     'get_latency'}) do
    test_run:cmd("push filter '"..v..": .*' to '"..v..": <"..v..">'")
end;
---
...
test_run:cmd("setopt delimiter ''");
---
- true
...
box_info_sort(box.info.vinyl())
---
- - compaction:
    - '0':
      - branch_age: 0
      - branch_age_period: 0
      - branch_age_wm: 0
      - branch_prio: 2
      - branch_wm: 10485760
      - compact_mode: 0
      - compact_wm: 2
      - gc_period: 60
      - gc_prio: 1
      - gc_wm: 30
      - lru_period: 0
      - lru_prio: 0
      - mode: 3
    - '80':
      - branch_age: 0
      - branch_age_period: 0
      - branch_age_wm: 0
      - branch_prio: 0
      - branch_wm: 0
      - compact_mode: 0
      - compact_wm: 4
      - gc_period: 0
      - gc_prio: 0
      - gc_wm: 0
      - lru_period: 0
      - lru_prio: 0
      - mode: 2
  - db:
    - 512:0:
      - branch_avg: 1
      - branch_count: 1
      - branch_histogram: '[1]:1 '
      - branch_max: 1
      - count: 2
      - count_dup: 0
      - memory_used: 58
      - node_count: 1
      - page_count: 1
      - read_cache: 0
      - read_disk: 0
      - size: 232
      - size_uncompressed: 232
      - temperature_avg: 0
      - temperature_histogram: '[0]:1-1 '
      - temperature_max: 0
      - temperature_min: 0
  - memory:
    - limit: 53687091
    - used: 58
  - metric:
    - lsn: 5
    - nsn: 1
  - performance:
    - cursor: 1
    - cursor_latency: <cursor_latency>
    - cursor_ops: 0 4 4.0
    - cursor_read_cache: 0 0 0.0
    - cursor_read_disk: 0 0 0.0
    - delete: 0
    - delete_latency: 0 0 0.0
    - documents: 2
    - documents_used: 58
    - get: 1
    - get_latency: <get_latency>
    - get_read_cache: 0 0 0.0
    - get_read_disk: 0 0 0.0
    - set: 0
    - set_latency: 0 0 0.0
    - tx: 2
    - tx_active_ro: 0
    - tx_active_rw: 0
    - tx_conflict: 0
    - tx_gc_queue: 0
    - tx_latency: <tx_latency>
    - tx_lock: 0
    - tx_ops: 0 1 1.0
    - tx_rollback: 0
    - upsert: 0
    - upsert_latency: 0 0 0.0
  - scheduler:
    - gc_active: 0
    - lru_active: 0
    - zone: '0'
  - vinyl:
    - build: <build>
    - path: <path>
    - version: 2.1.1
    - version_storage: 2.1.1
...
test_run:cmd("clear filter")
---
- true
...
space:drop()
---
...
test_run:cmd('switch default')
---
- true
...
test_run:cmd("stop server vinyl_info")
---
- true
...
test_run:cmd("start server vinyl_info")
---
- true
...
test_run:cmd("setopt delimiter ';'")
