-- test passing arguments in and out created fiber
--# setopt delimiter ';'
function y()
    box.fiber.detach('started')
    space = box.space['tweedledum']
    while true do
        space:replace('test', os.time())
        box.fiber.sleep(0.001)
    end
end;
