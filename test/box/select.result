--# setopt delimiter ';'
function test(idx, ...)
    local t1 = idx:select_ffi(...)
    local t2 = idx:select_luac(...)
    if msgpack.encode(t1) ~= msgpack.encode(t2) then
        return 'different result from select_ffi and select_luac', t1, t2
    end
    return t1
end;
